<?xml version="1.0"?>
<root length="3000">
    <!-- Possible tags 
        - sections : 
            - section :
                - id attribute
                - x number attribute
                - y number attribute
                - prop :
                    - name attribute
                    - x number attribute
                    - y number attribute
                    - pos : 
                        - x number attribute
                        - y number attribute
                    - scale
                        - x number attribute
                        - y number attribute
                    - vel
                        - x number attribute
                        - y number attribute
                    - path
                        - name attribute
                            - possible values: "straight"
                    - layer
                        - value attribute
                            - possible values: "background", "center", "foreground"
                    - solid
                        - value bool attribute
                    - collidable
                        - value bool attribute
                    - destructable
                        - value bool attribute
                    - pickup
                        - value bool attribute
        - grid :
            - random bool attribute
            - track :
                - id attribute
                - x number attribute
                - y number attribute
    -->

   <sections>
      <section id="1" x="0" y="0">
         <prop name="BG_Krat" x="100" y="200">
            <pos x="50" y="50" />
            <scale x="100" y="100" />
            <layer value="background" />
            <path value="straight" />
            <solid value="false" />
            <collidable value="false" />
            <pickup value="false" />
         </prop>
      </section>
   </sections>
   <!--
   <grid random="false">
      <track id="1" x="0" y="0" />
   </grid>-->
</root>